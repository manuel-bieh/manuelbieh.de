{
    "plugins": [
        "postcss-flexbugs-fixes",
        [
            "@csstools/postcss-global-data",
            {
                "files": [
                    "node_modules/@manuel-bieh/design-system/src/assets/css/media.css",
                    "node_modules/@manuel-bieh/design-system/src/assets/css/variables.css"
                ]
            }
        ],
        "postcss-nested",
        "postcss-custom-media",
        [
            "postcss-preset-env",
            {
                "autoprefixer": {
                    "flexbox": "no-2009"
                },
                "stage": 3,
                "features": {
                    "custom-properties": false
                }
            }
        ]
    ]
}
